a = [1, 2, 3]
total = 5
s = [0] * 5
s[0] = 0
for i in range(1, 6):
    print('value of i =%d'%i)
    test = []
    for j in range(4):
        if i >= a[j]:
            test.append(s[i-a[j]])
            print('test = ',test)
    print('***check***')
    s[i] = min(test)+1
    print('%d and %d ='%(i, s[i]))
for i in range(1,6):
    print(s[i])