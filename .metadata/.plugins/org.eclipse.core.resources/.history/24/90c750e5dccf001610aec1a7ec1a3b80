a = [1, 2, 3]
total = 5
s = [0] * 5
s[0] = 0
for i in range(1, 6):
    test = []
    for j in range(3):
        if i >= a[j]:
            test.append(s[i-a[j]])
            print('test = ',test)
    s[i] = min(test)+1
for i in range(1,6):
    print(s[i])