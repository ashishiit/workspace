class Link:
    def __init__(self,data):
        self.next = None
        self.data = data
class BinaryToDecimal:
    def __init__(self):
        self.first = None
    def InsertFirst(self,data):
        NewLink = Link(data)
        NewLink.next = self.first
        self.first = NewLink
    