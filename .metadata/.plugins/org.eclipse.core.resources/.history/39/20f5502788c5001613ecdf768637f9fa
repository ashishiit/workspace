class Link:
    def __init__(self,data):
        self.next = None
        self.data = data
class Merge2SortedLists:
    def __init__(self):
        self.first1 = None
        self.first2 = None
    def InsertFirst(self,data):
        NewLink = Link(data)
        NewLink.next = self.first1
        self.first1 = NewLink
    def Display(self):
        current1 = self.first1
        while current1 is not None:
            print(current1.data)
            current1 = current1.next
            
        
    
    